{
  "name": "StreamDrone",
  "version": "1.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --verbose",
    "ios": "react-native run-ios",
    "android": "cd ./android && ./gradlew app:assembleDevDebug && ./gradlew installDevDebug",
    "android-debug": "react-native run-android --variant=devDebug",
    "android-release": "react-native run-android --variant=devRelease",
    "android-bundle-js": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android-bundle-aab-dev": "cd ./android && ./gradlew app:bundleDevRelease",
    "android-bundle-aab-prod": "cd ./android && ./gradlew app:bundleProdRelease",
    "getApkFromBundle": "bundletool build-apks --bundle=Downloads/app.aab --output=Downloads/out_bundle_archive_set.apks --ks=ReactNativeProjects/trocr/android/app/trocr-release.keystore --ks-key-alias=trocr-release --ks-pass=pass:trocr34",
    "installApk": "bundletool install-apks --apks=Downloads/out_bundle_archive_set.apks",
    "keyHashForFb": "keytool -exportcert -alias devRelease -keystore android/app/trocr-release.keystore | openssl sha1 -binary| openssl base64",
    "firebaseDsymsForApple": "ios/Pods/FirebaseCrashlytics/upload-symbols -gsp ios/GoogleService-Info.plist -p ios appDsyms.zip",
    "detox-pixel": "detox test --configuration android.device.debug"
  },
